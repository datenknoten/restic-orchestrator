{
  "name": "restic-orchestrator",
  "version": "0.2.1",
  "description": "an orchestrator for restic backups on multiple hosts",
  "repository": "https://github.com/datenknoten/restic-orchestrator",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -p tsconfig.json",
    "start": "ts-node src/main.ts",
    "compile": "tsc -p tsconfig.json",
    "build": "cd dist && nexe --output restic-orchestrator main.js",
    "generate-config-schema": "ts-node src/generate-config-schema.ts"
  },
  "keywords": [
    "restic",
    "backup",
    "typescript",
    "node"
  ],
  "author": "Tim Schumacher",
  "license": "Apache-2.0",
  "dependencies": {
    "ajv": "5.5.0",
    "appdirectory": "0.1.0",
    "caporal": "0.8.0",
    "fs-extra": "4.0.2",
    "typescript-json-schema": "0.20.0",
    "winston": "2.4.0",
    "shelljs": "0.7.8"
  },
  "devDependencies": {
    "@types/node": "8.0.53",
    "@types/winston": "2.3.7",
    "@types/fs-extra": "4.0.5",
    "@types/shelljs": "0.7.6",
    "typescript": "2.6.1",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "nexe": "2.0.0-rc.20"
  }
}
